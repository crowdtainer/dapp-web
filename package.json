		"@sveltejs/adapter-auto": "1.0.2",
		"@sveltejs/adapter-node": "^1.1.6",
		"@sveltejs/kit": "^1.3.10",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.8",
		"@typechain/ethers-v5": "^10.1.1",
		"@types/node": "^18.7.14",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
